<section class="client-logos {{ background_color }}">
  <div class="container">
    <div class="row">
      <div class="col">
        {% if kicker %}
          <div class="title h4">
            {{ kicker }}
          </div>
        {% endif %}
        {% if title %}
          <div class="title h1">
            {{ title }}
          </div>
        {% endif %}
        {{ renderComponent('LogosRow') }}
      </div>
    </div>
  </div>
</section>
